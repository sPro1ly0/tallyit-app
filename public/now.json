{
    "version": 2,
    "alias": "sbecka8111-tallyit-app",
    "name": "tallyit",
    "routes": [
        { "handle": "filesystem" },
        { "src": "/.*", "dest": "/index.html" }
    ],
    "env": {
        "REACT_APP_TOKEN_KEY": "@react-app-token-key",
        "REACT_APP_API_BASE_URL": "@react-app-api-base-url"
    },
    "build": {
        "env": {
            "REACT_APP_TOKEN_KEY": "@react-app-token-key",
            "REACT_APP_API_BASE_URL": "@react-app-api-base-url"
        }
    }
}